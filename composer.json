{
    "name": "jdecool/doctrine-nullable-embeddable",
    "description": "Implement nullable embeddables in Doctrine entities",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "contact@jdecool.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.9.0 || ^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.55",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "symfony/cache": "^5.4 || ^6 || ^7"
    },
    "autoload": {
        "psr-4": {
            "JDecool\\DoctrineNullableEmbeddable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDecool\\DoctrineNullableEmbeddable\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        }
    }
}
